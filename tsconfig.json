{
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "target": "es6",
    "skipLibCheck": true,
  "baseUrl": "src",
  "strict": true,
  "outDir": "./src/dist",
  "sourceMap": true,
  "noImplicitAny": true,
  "esModuleInterop": true,
  "rootDir": "./src",
  "allowSyntheticDefaultImports": true
  // "allowImportingTsExtensions": true
  },
  "include": ["src/**/*","**/*.test.ts",],
  "exclude": ["node_modules", "dist"]
  }
  