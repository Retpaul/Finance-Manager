{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "node",
    "target": "es6",
    "skipLibCheck": true,
  "baseUrl": "src",
  "strict": true,
  "outDir": "./dist",
  "sourceMap": true,
  "noImplicitAny": true,
  "rootDir": "./src",
  "allowSyntheticDefaultImports": true
  // "allowImportingTsExtensions": true
  },
  "include": ["src/**/*","**/*.test.ts", "src/test/integration"],
  "exclude": ["node_modules", "dist"]
  }
  